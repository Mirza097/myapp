MDScreen:
    name: "pre-splash"
    MDFloatLayout:
        md_bg_color: 226/255, 0, 48/255, 1
        Image:
            source: "assets/111.jpg"
            size_hint: .26, .26
            pos_hint: {"center_x":.5,"center_y":.5}
            canvas.before:
                Color:
                    rgb: 1, 1, 1, 1
                Ellipse:
                    size: 130, 130
                    pos: 110, 225

    MDLabel:
        text: "Autonomous Car"
        pos_hint: {"center_x":.5,"center_y":.2}
        halign: "center"
        theme_text_color: "Custom"
        text_color: 1, 1, 1, 1
        font_size: "35sp"

        
    MDLabel:
        text: "The complete Autonomous"
        pos_hint: {"center_x":.5,"center_y":.15}
        halign: "center"
        theme_text_color: "Custom"
        text_color: 1, 1, 1, 1
        font_size: "13sp"
